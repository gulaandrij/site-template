{
    "name": "",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "friendsofsymfony/rest-bundle": "dev-master",
        "friendsofsymfony/user-bundle": "^2.1",
        "fzaninotto/faker": "^1.7",
        "jms/serializer-bundle": "^3.0",
        "lexik/jwt-authentication-bundle": "^2.5",
        "nelmio/api-doc-bundle": "^3.3",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "sentry/sentry-symfony": "^2.0",
        "snc/redis-bundle": "^2.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.2.*",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "gulaandrij/coding-standard": "^1.0.0",
        "liip/functional-test-bundle": "2.x-dev",
        "mybuilder/phpunit-accelerator": "dev-master",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-deprecation-rules": "^0.10.2",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpunit/phpunit": "^7.4",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "apcu-autoloader": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01CC320Z8FF3J26R3JHZFVDD6M",
            "allow-contrib": true,
            "require": "4.2.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
